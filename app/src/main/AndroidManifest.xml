<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anashamidkh.parkingsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AuthenticationActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".UserDescription" />
        <activity android:name=".SubmitFeedbackActivity" />
        <activity android:name=".ShowListActivity" />
        <activity android:name=".FeedbackDescription" />
        <activity android:name=".AreaActivity" />
        <activity android:name=".SlotsActivity" />
        <activity android:name=".CancelBookingActivity" />
        <activity android:name=".MyBookingsActivity" />
        <activity android:name=".ViewBookingsActivity" />
        <activity android:name=".BookingDescriptionActivity" />
    </application>

</manifest>